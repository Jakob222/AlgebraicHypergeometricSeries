Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "HyperSol", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"HyperSol", "[", 
   RowBox[{"a_", ",", "b_", ",", "c_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", "\[Beta]", ",", "\[Alpha]2", ",", "\[Beta]2", ",", "l", 
      ",", "d", ",", "coefs", ",", "A", ",", "p", ",", "i", ",", 
      RowBox[{"Sol", "=", 
       RowBox[{"{", "}"}]}], ",", "L"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Alpha]", "=", 
      RowBox[{"1", "-", "c"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Beta]", "=", 
      RowBox[{"c", "-", "a", "-", "b"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"A", "=", 
      RowBox[{"Tuples", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Which", "[", 
          RowBox[{
           RowBox[{"\[Alpha]", "\[Element]", 
            TemplateBox[{},
             "Integers"]}], ",", 
           RowBox[{"{", 
            RowBox[{"Min", "[", 
             RowBox[{"\[Alpha]", ",", "0"}], "]"}], "}"}], ",", 
           RowBox[{"\[Alpha]", "\[Element]", 
            TemplateBox[{},
             "Rationals"]}], ",", 
           RowBox[{"{", 
            RowBox[{"\[Alpha]", ",", "0"}], "}"}], ",", "True", ",", 
           RowBox[{"{", "0", "}"}]}], "]"}], ",", 
         RowBox[{"Which", "[", 
          RowBox[{
           RowBox[{"\[Beta]", "\[Element]", 
            TemplateBox[{},
             "Integers"]}], ",", 
           RowBox[{"{", 
            RowBox[{"Min", "[", 
             RowBox[{"\[Beta]", ",", "0"}], "]"}], "}"}], ",", 
           RowBox[{"\[Beta]", "\[Element]", 
            TemplateBox[{},
             "Rationals"]}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "\[Beta]"}], "}"}], ",", "True", ",", 
           RowBox[{"{", "0", "}"}]}], "]"}]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "A", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "=", 
         RowBox[{
         "A", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], ";", 
        RowBox[{"d", "=", 
         RowBox[{"Floor", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"Min", "[", 
             RowBox[{
              RowBox[{"Re", "[", "a", "]"}], ",", 
              RowBox[{"Re", "[", "b", "]"}]}], "]"}]}], "-", "\[Alpha]", "-", 
           "\[Beta]"}], "]"}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"d", "<", "0"}], ",", 
          RowBox[{"Continue", "[", "]"}]}], "]"}], ";", 
        RowBox[{"coefs", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Module", "[", 
            RowBox[{
             RowBox[{"{", "pc", "}"}], ",", "pc"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"d", "+", "1"}], "}"}]}], "]"}]}], ";", 
        RowBox[{"p", "=", 
         RowBox[{"FromDigits", "[", 
          RowBox[{"coefs", ",", "x"}], "]"}]}], ";", 
        RowBox[{"l", "=", 
         RowBox[{"CoefficientList", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                SuperscriptBox[
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"-", "1"}], "+", "x"}], ")"}], "2"], " ", 
                SuperscriptBox["\[Alpha]", "2"]}], "+", 
               RowBox[{"a", " ", "x", " ", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"-", "1"}], "+", "x"}], ")"}], " ", 
                   RowBox[{"(", 
                    RowBox[{"b", "+", "\[Alpha]"}], ")"}]}], "+", 
                  RowBox[{"x", " ", "\[Beta]"}]}], ")"}]}], "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"-", "1"}], "+", "x"}], ")"}], " ", "\[Alpha]", " ", 
                RowBox[{"(", 
                 RowBox[{"1", "-", "c", "+", 
                  RowBox[{"b", " ", "x"}], "+", 
                  RowBox[{"2", " ", "x", " ", "\[Beta]"}]}], ")"}]}], "+", 
               RowBox[{"x", " ", "\[Beta]", " ", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"-", "c"}], "+", 
                  RowBox[{"x", " ", 
                   RowBox[{"(", 
                    RowBox[{"b", "+", "\[Beta]"}], ")"}]}]}], ")"}]}]}], 
              ")"}], " ", "p"}], "+", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "1"}], "+", "x"}], ")"}], " ", "x", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "c"}], "-", 
               RowBox[{"2", " ", "\[Alpha]"}], "+", 
               RowBox[{"x", " ", 
                RowBox[{"(", 
                 RowBox[{"1", "+", "a", "+", "b", "+", 
                  RowBox[{"2", " ", "\[Alpha]"}], "+", 
                  RowBox[{"2", " ", "\[Beta]"}]}], ")"}]}]}], ")"}], " ", 
             RowBox[{
              SubscriptBox["\[PartialD]", "x"], "p"}]}], "+", 
            RowBox[{
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                RowBox[{"-", "1"}], "+", "x"}], ")"}], "2"], " ", 
             SuperscriptBox["x", "2"], " ", 
             RowBox[{
              SubscriptBox["\[PartialD]", 
               RowBox[{"{", 
                RowBox[{"x", ",", "2"}], "}"}]], "p"}]}]}], ",", "x"}], 
          "]"}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"l", "\[Equal]", 
           RowBox[{"{", "}"}]}], ",", 
          RowBox[{"l", "=", 
           RowBox[{"{", "0", "}"}]}]}], "]"}], ";", 
        RowBox[{"L", "=", 
         RowBox[{"NullSpace", "[", 
          RowBox[{"RowReduce", "[", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"Coefficient", "[", 
              RowBox[{
               RowBox[{
               "l", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
               ",", 
               RowBox[{
               "coefs", "\[LeftDoubleBracket]", "j", 
                "\[RightDoubleBracket]"}]}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "1", ",", 
               RowBox[{"Length", "[", "l", "]"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", 
               RowBox[{"d", "+", "1"}]}], "}"}]}], "]"}], "]"}], "]"}]}], ";", 
        RowBox[{"Sol", "=", 
         RowBox[{"Sol", "\[Union]", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Together", "[", 
              RowBox[{
               SuperscriptBox["x", "\[Alpha]"], " ", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{"1", "-", "x"}], ")"}], "\[Beta]"], " ", 
               RowBox[{"FromDigits", "[", 
                RowBox[{"#1", ",", "x"}], "]"}]}], "]"}], "&"}], ")"}], "/@", 
           "L"}]}]}], ";"}]}], "]"}], ";", 
     RowBox[{"Return", "[", "Sol", "]"}]}]}], "]"}]}]}], "Input",
